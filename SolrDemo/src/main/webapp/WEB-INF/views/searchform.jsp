<form action="searchResult" name="search" method="post">
	<div id="searchContainer">
		<label>Search Keyword</label> <input name="keyword" type="text"
			id="searchBox">
		<button type="submit">Search</button>
		<div>
			Enable Fuzzy Search\
			<input type="checkbox" name="fuzzySearch" value="true">
			Distance <input type="text" name="distance" size="2">
		</div>
	</div>
</form>
